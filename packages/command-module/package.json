{
  "name": "@lunar-flight-v/command-module",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.1",
  "author": "Marcin Mazurowski @ _KTW-STUDIO <hello@ktwstudio.com>",
  "description": "@lunar-lander/apollo is monorepo for ...",
  "homepage": "",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "npm install",
    "dev": "NODE_ENV=development nodemon --watch \"src/**/*.ts\" --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "test:watch": "NODE_ENV=test jest --config ./jest.config.js --watch",
    "lint": "eslint --ignore-path ../../.eslintignore --ext .ts \"src/**/*\"",
    "lint:fix": "eslint --fix",
    "format": "prettier --config ./../../.prettierrc --write \"src/**/*\"",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "gitHead": "e4c73d3a993559258cee8be2536d551cbf067435"
}
